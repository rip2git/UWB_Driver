#ifndef COMHANDLER_H_
#define COMHANDLER_H_



#include "UserPack.h"
#include "COMPort.h"



class COMHandler {
public:
	enum class RESULT {
		Error = -1,
		Success
	};

	COMHandler();
	COMHandler::RESULT Send(UserPack *pack);
	COMHandler::RESULT Receive(UserPack *pack);
private:
	COMPort port;
};



#endif /* COMHANDLER_H_ */
