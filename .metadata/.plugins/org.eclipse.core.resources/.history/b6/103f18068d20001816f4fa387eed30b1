#ifndef USERPACK_H_
#define USERPACK_H_



struct UserPack {

	enum class COMMAND : unsigned char {
		Distance = 1,
		Data
	};
	COMMAND Command;
	unsigned char DestinationID;
	unsigned char TotalSize;
	unsigned char *Data;
	void ToBytes(unsigned char *buffer) const;
	void ToStruct(const unsigned char *buffer);
};



#endif /* USERPACK_H_ */
